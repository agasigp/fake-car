{
    "name": "pelmered/fake-car",
    "type": "library",
    "description": "Fake-Car is a Faker provider that generates fake car data for you.",
    "keywords": [
        "faker",
        "car",
        "vehicle",
        "fake-car",
        "car-faker"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Elmered",
            "email": "peter@elmered.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "fzaninotto/faker": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Faker\\Provider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faker\\Tests\\Provider\\": "tests/Faker"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit -c tests/phpunit.xml"
        ],
        "test-coverage": [
            "./vendor/bin/phpunit --coverage-text -c tests/phpunit.xml"
        ]
    }
}
